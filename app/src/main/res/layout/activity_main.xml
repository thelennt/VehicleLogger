<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<Button android:id="@+id/AddLog" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Add Log"
		tools:layout_constraintTop_creator="1"
		tools:layout_constraintRight_creator="1"
		app:layout_constraintRight_toRightOf="@+id/GetLog"
		app:layout_constraintTop_toBottomOf="@+id/GetLog"
		tools:layout_constraintLeft_creator="1"
		app:layout_constraintLeft_toLeftOf="@+id/GetLog" />
	<Button android:id="@+id/GetLog" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Get Log"
		tools:layout_constraintRight_creator="1"
		tools:layout_constraintBottom_creator="1"
		app:layout_constraintBottom_toBottomOf="@+id/constraintLayout"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_constraintLeft_creator="1"
		android:layout_marginBottom="43dp"
		app:layout_constraintLeft_toLeftOf="parent" />
	<EditText android:ems="10" android:id="@+id/LogInput" android:inputType="text" android:layout_height="38dp" android:layout_width="0dp"
		tools:layout_constraintTop_creator="1"
		tools:layout_constraintRight_creator="1"
		android:layout_marginStart="33dp"
		android:layout_marginEnd="33dp"
		app:layout_constraintRight_toRightOf="parent"
		android:layout_marginTop="73dp"
		tools:layout_constraintLeft_creator="1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginLeft="33dp"
		android:layout_marginRight="33dp" />
	<TextView android:id="@+id/LogOutput" android:layout_height="0dp" android:layout_width="0dp" android:text="Log Data will be here"
		tools:layout_constraintTop_creator="1"
		tools:layout_constraintRight_creator="1"
		tools:layout_constraintBottom_creator="1"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		android:layout_marginTop="8dp"
		app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
		tools:layout_constraintLeft_creator="1"
		android:layout_marginBottom="2dp"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintVertical_bias="0.0" />

	<android.support.constraint.ConstraintLayout
		android:id="@+id/constraintLayout"
		android:layout_width="0dp"
		android:layout_height="396dp"
		android:layout_marginTop="-168dp"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_constraintLeft_creator="1"
		tools:layout_constraintRight_creator="1"
		tools:layout_constraintTop_creator="1">

		<TextView
			android:id="@+id/textView"
			android:layout_width="344dp"
			android:layout_height="31dp"
			android:layout_marginTop="16dp"
			android:text="Car Logging App"
			app:layout_constraintHorizontal_bias="0.582"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_constraintLeft_creator="1"
			tools:layout_constraintRight_creator="1"
			tools:layout_constraintTop_creator="1" />

		<TextView
			android:id="@+id/VIN"
			android:layout_width="333dp"
			android:layout_height="44dp"
			android:layout_marginTop="144dp"
			android:text="Car VIN"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_constraintLeft_creator="1"
			tools:layout_constraintRight_creator="1"
			tools:layout_constraintTop_creator="1" />

		<TextView
			android:id="@+id/Balance"
			android:layout_width="333dp"
			android:layout_height="44dp"
			android:layout_marginTop="208dp"
			android:text="Balance"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_constraintLeft_creator="1"
			tools:layout_constraintRight_creator="1"
			tools:layout_constraintTop_creator="1" />
	</android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>
